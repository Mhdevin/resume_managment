<%= simple_form_for(resume) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :personality%>
    <%= f.input :graduation_time, as: :date %>
    <%= f.input :familiar_frame %>
    <%= f.input :apply_position %>
    <%= f.input :project_experience %>
    <%= f.input :self_evaluation %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= link_to '取消',resumes_path, class: 'button button-rounded button-flat' %>
  </div>
<% end %>
