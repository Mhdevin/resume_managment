<%= simple_form_for(@resume) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :personality %>
    <%= f.input :graduation_time %>
    <%= f.input :familiar_frame %>
    <%= f.input :apply %>
    <%= f.input :position %>
    <%= f.input :project_experience %>
    <%= f.input :self_evaluation %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
